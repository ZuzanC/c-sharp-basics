using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.IO;

namespace testing
{
    class Program
    {


        static void Main(string[] args)
        {
            int[] one =  { 1, 3, -5, 4 };
            int[] two =  { 1, 4, -5, -2 };
            int result;


            for (int i = 0; i < one.Length; i++)
            {

                {
                    result = one[i] * two[i];
                    Console.WriteLine(result);



                }
            }



            Console.ReadKey();


        }
    }
}
